<script>$Nav.declare("amazon.popover.tooltip");</script>
















<script type="text/javascript">
(function () {
        var returnUrl = null;
        var custrecReturnURL = function($) {
            var newPathName = "https://" + window.location.host;
            var pathArray = window.location.pathname.split('/');
            var urlParameters = window.location.search.substring(1).split('&');
            if ($.browser && $.browser.msie) {
                var wholeURL = $(location).attr('href');
                var tmpSplit = wholeURL.split('?');
                urlParameters = tmpSplit[tmpSplit.length - 1].split('&');
            }
            var urlPattern = /^ref=/;
            for ( i=0; i<pathArray.length; i++) {
                if (!urlPattern.test(pathArray[i]) && pathArray[i] != "") {
                    newPathName += "/" + pathArray[i];
                }
            }
            var paramPattern = /^ref_=/;
            newPathName += "?";
            for ( i=0; i<urlParameters.length; i++) {
                if (!paramPattern.test(urlParameters[i])) {
                    newPathName += urlParameters[i] + "&";
                }
            }
            newPathName = newPathName.replace(/undefined&$/,"");
            newPathName = newPathName.replace(/[&?]$/,"");
            return newPathName;
        }
   // replace return URL on client side. It works, do not touch it
        var custrecReplaceReturnURL = function(originalLink, signInType, refTag) {
            //signInType: 1 - inline ; 2 - lightbox ; 3 - redirect
        	var newLink = "";
        	var URLs = originalLink.split('?');
        	newLink = URLs[0] + "?";
        	var pathArray = URLs[1].split( '&' );
        	var paramPattern = null;
        	paramPattern = /^openid.return_to=/;
            for ( i=0; i<pathArray.length; i++) {
                if (!paramPattern.test(pathArray[i])) {
                    newLink += pathArray[i] + "&";
                } else {
                    var parameters = pathArray[i].split('=');
                    var tmpReturnURL = decodeURIComponent(parameters[1]);
                    var returnParameters = tmpReturnURL.split('?');
                    newLink += parameters[0] + '=' + encodeURIComponent(returnUrl + '&ref_=' + refTag); 
                }
            }
            newLink = newLink.replace(/[&?]$/,"");
            return newLink;
        }
        function redirectFun(event) {
               returnUrl = custrecReturnURL(event.data.jQuery);
               event.data.directLink = custrecReplaceReturnURL(event.data.directLink, 3, 'custrec_signin_redirect_same_page');
                window.location=event.data.directLink;
                event.preventDefault ? event.preventDefault() : event.returnValue = false;
            }
        function replaceLink(target, $, directLink) {
            $(target).bind('touchstart click',{directLink:directLink,jQuery:$},redirectFun);
            if($('body').delegate)
            {
              $('body').delegate(target,'touchstart click',{directLink:directLink,jQuery:$},redirectFun);
            }
        }
        function attachLink($)
        {
        if (1==0) {
          return;
        }
            var directLink = "https://www.amazon.com/ap/signin/183-6390091-2270011?_encoding=UTF8&openid.assoc_handle=usflex&openid.claimed_id=http%3A%2F%2Fspecs.openid.net%2Fauth%2F2.0%2Fidentifier_select&openid.identity=http%3A%2F%2Fspecs.openid.net%2Fauth%2F2.0%2Fidentifier_select&openid.mode=checkid_setup&openid.ns=http%3A%2F%2Fspecs.openid.net%2Fauth%2F2.0&openid.ns.pape=http%3A%2F%2Fspecs.openid.net%2Fextensions%2Fpape%2F1.0&openid.pape.max_auth_age=0&openid.return_to=https%3A%2F%2Fwww.amazon.com%2Freview%2FB00BP5KOPA%3Fie%3DUTF8%26pageNumber%3D2%26ref_%3Dcustrec_signin_redirect_same_page%26showViewpoints%3D0%26sortBy%3DbyRankDescending";
            //var domArray= $("#sign-in-tooltip-body .a-button a , #nav_your_account_flyout a.nav-action-button , .cust-rec-tooltip-nav-flyout a.nav-action-button");
            //returnUrl = custrecReturnURL($);
            replaceLink("#sign-in-tooltip-body .a-button a , #nav_your_account_flyout a.nav-action-button , .cust-rec-tooltip-nav-flyout a.nav-action-button", $, directLink);
        }

       $Nav.when( '$', 'page.domReady','amazon.popover.tooltip','flyout.content').run('custrec_nav_redirect', function($) {
          attachLink($);
       });
})();
</script>








  






